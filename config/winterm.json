{
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
        {
            "command":
            {
                "action": "newTab"
            },
            "keys": "alt+shift+ctrl+t"
        },
        {
            "command":
            {
                "action": "duplicateTab"
            },
            "keys": "alt+shift+t"
        },
        {
            "command":
            {
                "action": "resizePane",
                "direction": "down"
            },
            "keys": "alt+shift+down"
        },
        {
            "command":
            {
                "action": "renameTab"
            },
            "keys": "alt+shift+ctrl+r"
        },
        {
            "command":
            {
                "action": "openTabRenamer"
            },
            "keys": "alt+shift+r"
        },
        {
            "command":
            {
                "action": "closePane"
            },
            "keys": "alt+shift+q"
        },
        {
            "command":
            {
                "action": "splitPane",
                "split": "horizontal",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+s"
        },
        {
            "command":
            {
                "action": "resizePane",
                "direction": "up"
            },
            "keys": "alt+shift+up"
        },
        {
            "command":
            {
                "action": "splitPane",
                "split": "vertical",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+v"
        },
        {
            "command":
            {
                "action": "resizePane",
                "direction": "left"
            },
            "keys": "alt+shift+left"
        },
        {
            "command":
            {
                "action": "resizePane",
                "direction": "right"
            },
            "keys": "alt+right"
        },
        {
            "command":
            {
                "action": "moveFocus",
                "direction": "down"
            },
            "keys": "alt+shift+j"
        },
        {
            "command":
            {
                "action": "moveFocus",
                "direction": "left"
            },
            "keys": "alt+shift+h"
        },
        {
            "command":
            {
                "action": "moveFocus",
                "direction": "right"
            },
            "keys": "alt+shift+l"
        },
        {
            "command":
            {
                "action": "moveFocus",
                "direction": "up"
            },
            "keys": "alt+shift+k"
        }
    ],
    "alwaysOnTop": false,
    "alwaysShowTabs": true,
    "confirmCloseAllTabs": true,
    "copyFormatting": "all",
    "copyOnSelect": true,
    "defaultProfile": "{61c54bbd-c2c6-5271-96e7-009a87ff44bf}",
    "disableAnimations": true,
    // Dynamic profiles
    "disabledProfileSources": 
    [
        "Windows.Terminal.Azure",
        "Windows.Terminal.PowershellCore"
    ],
    "initialCols": 100,
    "initialPosition": ",",
    "initialRows": 30,
    "largePasteWarning": true,
    "launchMode": "default",
    "multiLinePasteWarning": true,
    "profiles": 
    {
        "defaults": 
        {
            "acrylicOpacity": 0.5,
            "antialiasingMode": "cleartype",
            "bellStyle": "none",
            "closeOnExit": "graceful",
            "colorScheme": "Campbell",
            "cursorColor": "#FEFEFE",
            "cursorShape": "emptyBox",
            "fontFace": "Cascadia Mono",
            "fontSize": 9,
            "fontWeight": "normal",
            "hidden": false,
            "historySize": 9001,
            "padding": "8, 8, 8, 8",
            "scrollbarState": "visible",
            "snapOnInput": true,
            "startingDirectory": "%USERPROFILE%",
            "suppressApplicationTitle": true,
            "useAcrylic": false
        },
        "list": 
        [
            {
                "colorScheme": "Dracula",
                "commandline": "pwsh -NoLogo",
                "fontFace": "Cascadia Code PL",
                "guid": "{63fefd1b-654e-4712-a017-509ef6169fef}",
                "icon": "\ud83c\udf65",
                "name": "Pwsh",
                "tabTitle": "pwsh"
            },
            {
                "acrylicOpacity": 0.5,
                "colorScheme": "Dracula",
                "commandline": "pwsh -NoLogo",
                "fontFace": "Cascadia Code PL",
                "guid": "{38cde125-00f4-4397-bc83-a842702235f9}",
                "icon": "\ud83e\uddca",
                "name": "Pwsh (Acrylic)",
                "tabTitle": "pwsh",
                "useAcrylic": true
            },
            {},
            {}
        ]
    },
